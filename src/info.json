{
  "identifier": "eudic-wordbook-plugin",
  "version": "1.0.0",
  "category": "translate",
  "name": "欧路词典单词本",
  "summary": "Add words to Eudic word book",
  "icon": "",
  "author": "Moreality",
  "homepage": "https://blog.yuhaowin.com/2021-01-03-wordbook-bob-plugin",
  "appcast": "https://raw.githubusercontent.com/lincest/wordbook-bob-plugin/main/appcast.json",
  "minBobVersion": "1.12.0",
  "options": [
    {
      "identifier": "authorization",
      "type": "text",
      "title": "欧路词典认证信息",
      "textConfig": {
        "type": "secure",
        "placeholderText": "填写欧路词典 api token"
      }
    },
    {
      "identifier": "word_only",
      "type": "menu",
      "title": "仅保存单词",
      "desc": "是否仅保存「单词」或「词组」。",
      "defaultValue": "1",
      "menuValues": [
        {
          "title": "是",
          "value": "1"
        },
        {
          "title": "否",
          "value": "0"
        }
      ]
    },
    {
      "identifier": "wordbook_id",
      "type": "text",
      "title": "欧路单词本 ID",
      "desc": "填写欧路词典单词本 ID，可通过点击「验证」按钮获取。",
      "textConfig": {
        "type": "visible",
        "placeholderText": "点击「验证」获取已有单词本 ID，填入后再次点击「验证」确认是否正确。"
      }
    }
  ]
}